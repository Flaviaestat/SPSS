GET FILE = 'D:\Users\Flávia\Documents\Trabalho\Modelos Venda\Atualização Modelo DF\2008_05\BASE VENDAS MODELO_JAN06_25MAR08.SAV'.

STRING DIA_SEM (A4).
IF(SEG=1) DIA_SEM='SEG'.
IF(TER=1) DIA_SEM='TER'.
IF(QUA=1) DIA_SEM='QUA'.
IF(QUI=1) DIA_SEM='QUI'.
IF(SEX=1) DIA_SEM='SEX'.
IF(SAB=1) DIA_SEM='SAB'.
IF(DOM=1) DIA_SEM='DOM'.
FREQUENCIES DIA_SEM.

TEMPORARY.
SELECT IF(LOJA=3).
EXAMINE
  VARIABLES=venda BY DIA_SEM
  /PLOT  STEMLEAF
  /PERCENTILES(5,10,25,50,75,90,95) HAVERAGE
  /STATISTICS DESCRIPTIVES EXTREME
  /CINTERVAL 95
  /MISSING LISTWISE
  /NOTOTAL.



